<form action="/students" method="post">
  <label for="first-name">First Name:</label>
  <input type="text" id="first-name" name="first_name">
  
  <label for="second-name">Second Name:</label>
  <input type="text" name="second_name" id="second-name">
  
  <label for="house-id">House:</label>
  <select id="house-id" name="house_id">
    <option value=""selected disabled>What is the house?</option>
    <%@houses.each do |house| %>
    <option value="<%=house.id%>"><%=house.name%></option>
    <%end%>
  </select>
  
  <label for="age">Age:</label>
  <input type="number" name="age" id="age">
  
  <input type="submit" name="" value="Add new student">
  
</form>
